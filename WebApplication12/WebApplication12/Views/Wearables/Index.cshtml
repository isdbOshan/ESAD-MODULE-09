@model IEnumerable<Wearable>
@{
    ViewData["Title"] = "Index";
}
@*new Wearable {ProductId=1,ProductName="Sunglass",UnitPrice=250.00M, Size=34, Color="Black"},*@
<div class="row">
    <div class="col-12">
        <h3>Wearable Product</h3>
        <table class="table table-bordered">
            <thead>
                <tr>
                    <th>Product Name</th>
                    <th>Price</th>
                    <th>Size</th>
                    <th>Color</th>
                </tr>
            </thead>
            <tbody>
                @foreach(var p in Model)
                {
                     <tr>
                    <td>@p.ProductName</td>
                    <td>@p.UnitPrice</td>
                    <td>@p.Size</td>
                    <td>@p.Color</td>
                </tr>
                }
            </tbody>
        </table>
    </div>
</div>

